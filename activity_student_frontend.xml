        android:layout_height="wrap_content"
        android:text="Hindi" />
    <Button
        android:id="@+id/b4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Telugu" />
    <Button
        android:id="@+id/b5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="English" />
    <Button
        android:id="@+id/b6"
        android:layout_width="match_parent"
